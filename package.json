{
  "name": "cerebro-gest",
  "version": "1.0.0",
  "description": "Multi-Tenant SaaS Infrastructure",
  "private": true,
  "workspaces": [
    "apps/*",
    "shared",
    "infra/*"
  ],
  "scripts": {
